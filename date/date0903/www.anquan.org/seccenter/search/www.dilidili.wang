	$(document).ready(function(){
		var sites = new Array();
		var $s_form = $("#global-nav-search");
		$.getJSON('/seccenter/random_sites/',function(data){
			 _.each(data, function(s){ sites.push(s['_id']);});
			$("#domainText").typeahead({
				source: sites,
				onselect: function(obj){
				$s_form.submit();
				}
			});
			
			$("#home_search").typeahead({
				source: sites,
				onselect: function(obj){
				$("#home-search-form").submit();
