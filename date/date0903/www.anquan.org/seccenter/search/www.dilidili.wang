<script type="text/javascript">
var ss_mtip=".tip_container {display:none;z-index: 11000;position: fixed;_position:absolute;top:40%;left:50%;margin-left: -288px;text-align: left;color:#333;}";
GLOBAL.require.css(ss_mtip);
function mtip(tip_msg, stat, timeout){
		if(!tip_msg) return;
		$tip_container = $("#tip_container");
		var bWating = $tip_container.find(".mtip.waiting")[0];
		if(!bWating){
		$(".tip_container").css({
				"display":"none",
				"top":"40%",
				"opacity":"1"
			})
		}
		clearTimeout(window.mTipTimer);
		if(!timeout){
			timeout = 2000;
			if(window.G_MOBILE_FLAG){
				timeout = 800;
			}
		}
		$tip_container.find(".mtip").removeClass("success").removeClass("error").removeClass("warning").removeClass("waiting").addClass(stat);
        $tip_container.find("#tip>span").html(tip_msg);
		if(!bWating){
			//计算当前MTIP位置
			var distance = 20,bIE6 = (typeof document.body.style.maxWidth ==="undefined");
			//执行动画 淫荡的IE6.
			if(bIE6){
				//IE6 初始化对话框
				var IE6Y = (document.documentElement.scrollTop||document.body.scrollTop)+300;
				$tip_container.css("top",IE6Y+"px");
			}
			$tip_container.show();
			var nTop = $tip_container.offset().top-(document.documentElement.scrollTop||document.body.scrollTop);
			$tip_container.css("top",parseInt(nTop)+"px");
			bIE6 && (nTop = IE6Y);
			var mWidth = $tip_container.outerWidth();
			//IE6 Mxwidth检测
			if(bIE6 && $tip_container.find(".mtip").outerWidth() > 250 ){
					$tip_container.find(".mtip").width(250);
			}
			$tip_container.css("marginLeft","-"+parseInt(mWidth)/2+"px");
			$tip_container.stop(true).animate({top:parseInt(nTop - distance)+"px"},{
				duration: 160,
				easing:"easeInQuad"
			})
		}
		var fnReset = function(){
			$tip_container.css({
				top:"40%",
				opacity:"1",
				display:"none"
			}).find(".mtip").removeClass("success").removeClass("error").removeClass("warning").removeClass("waiting");
		}
		var fnHideTip = function(){
			var nTop = $tip_container.offset().top-(document.documentElement.scrollTop||document.body.scrollTop);
			bIE6 && (nTop = IE6Y - distance);
			$tip_container.stop(true).animate({
				top:parseInt(nTop-40)+"px",
				opacity:"0"
				},{duration:300,easing:"easeInQuad",complete:function(){
					fnReset();	
				}
			})
		}
		//假设人能忍受的等待时间是15秒.
		if(stat =="waiting"){
			timeout = 15000;
		}
		window.mTipTimer = setTimeout(function(){
				fnHideTip();
		}, timeout);
		$tip_container.unbind("click").on("click",'.mclose',function(){
			fnHideTip();		
		})
		$tip_container.unbind('mouseover').mouseover(function(){
			clearTimeout(window.mTipTimer);	
		});
		$tip_container.unbind("mouseout").mouseout(function(){
			if(bWating) return false;
			window.mTipTimer = setTimeout(function(){
				fnHideTip();	
			}, timeout);
		});
}
function mtip_remove(){
	$("#tip_container").css({
		top:"40%",
		opacity:"1",
		display:"none"
	}).find(".mtip").removeClass("success").removeClass("error").removeClass("warning").removeClass("waiting");	
}
</script>

