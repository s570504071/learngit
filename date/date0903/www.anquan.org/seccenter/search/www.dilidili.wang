			//highlighter: function(item) {
			//	return item;
			//}
			});
			total = sites.length;
			var my_num= parseInt((1+Math.random()*(total)));
			var my_string = data[my_num]['_id'];
			while(my_string.length > 20){
				my_num= parseInt((1+Math.random()*(total)));
				my_string = data[my_num]['_id'];
			}
			$('#home_search').note_input(data[my_num]['_id']+' - 网站信誉一查便知');
		})
		$(".input input").focus(function(){
			$(this).siblings('label').css('display','none');
		}).blur(function(){
			if($(this).val() != ''){
				$(this).siblings('label').css('display','none');
			}
			else {
				$(this).siblings('label').css('display','block');
			}
		});
		$(".input input").bind('blur',function(){
			$(this).siblings('.msgr').remove();
		});
		$(".input").bind('click',function(){
			$(this).children('input').focus();
		});
		$("#login_btn").click(modal_login);
	//enter login
		$("#auth_form input").keydown(function(event){
			if(event.keyCode ==  13 && $("#sheet").is(":hidden") == false){
			$("#login_submit").click();
			}
		})
	});
	function hideSheet() {
		$("#login_btn").click();	
	}	
	function submitLogin() {
		var email = $("#login_email").val();
		var password = $("#login_password").val();
		var captcha = $("#captcha").val();
		var csrf = $("input[name='csrfmiddlewaretoken']").val();
		var rz_flag = $("#rz_flag").val();
		var domain = $("#domain").val();
		var success = false;
		var next_url = '//www.anquan.org';
		var url = $("#auth_form").attr("action");
		var my_next_url = $("#my_next_url").val();
		$.ajax({
			url: url,
			type:'POST',
			dataType: 'json',
			async:false,
			data: {
					'email':email,
					'password':password,
					'captcha':captcha,
					'csrfmiddlewaretoken': csrf,
					'rz_flag':rz_flag,
					'domain':domain,
					'next_url':my_next_url
			},
			success: function(result) {
				if(result.status == 'error') {
					if(result.msg)
						mtip(result.msg,"error");
					else
						mtip("未知错误，请重新提交试试","error");

					if(result.captcha_invalid){
						$("#captcha_div").show().find("a").trigger('click');
					}
				}
				else {
					success = true;
					next_url = result.next_url;
					window.location.href = next_url;
				}
			}
		}); 
		if(success) {
			window.location.href = next_url;
		}
	}

	function submitSearch(obj){
		var search_str = $.trim($(obj).find("input[name=domain]").val());
		var url = search_str.split(' ')[0],
		domain=url.replace(/http:\/\/|https:\/\//i,'').split('/')[0];
		var re=/[A-Za-z0-9]+\.[A-Za-z]+$/;
		if(domain.indexOf(':') !=-1) re=/[A-Za-z0-9]+\.[A-Za-z]+\:(\d+)$/;
		var rip=/(\d+)\.(\d+)\.(\d+).(\d+)/g;
		if(re.test(domain) || rip.test(domain)) {
			window.location.href = "/explore/search_domain/" + encodeURIComponent(url) + "/";
		}else if (search_str && search_str !== '网站信誉一查便知'){
			//search_str = search_str.split(' ')[0]; 
			search_str = search_str.slice(0, 100);  
			window.location.href = "/explore/search/" + search_str + "/";
		}else{
			mtip('请输入合法域名或想查询的关键词。', 'error');
		}
		return false;
	}
	function to_login(next_url){
		if(next_url){
			$("#qq_login_btn").attr('href','/login/?type=qq&act=auth&next_url=' + next_url);
			$("#weibo_login_btn").attr('href','/login/?type=weibo&act=auth&next_url=' + next_url);
			$("#baidu_login_btn").attr('href','/login/?type=baidu&act=auth&next_url=' + next_url);
			$("#auth_form").attr('action','/login/?type=email&next_url=' + next_url);
		}
		$("#sheet").find("#login_email").val('').end().find("#login_password").val('').end().modal('hide').modal('show');
	}	
